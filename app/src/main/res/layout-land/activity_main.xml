<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/ActivityMainLand_GlVr1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="@dimen/default_margin" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/ActivityMainLand_GlVr2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_end="@dimen/default_margin" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/ActivityMainLand_GlHr1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="@dimen/default_margin" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/ActivityMainLand_GlHr2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_end="@dimen/default_margin" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/ActivityMainLand_GlVr3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="@fraction/ActivityMainLand_GlVr3" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/ActivityMainLand_GlVr4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="@fraction/ActivityMainLand_GlVr4" />

    <FrameLayout
        android:id="@+id/ActivityMainLand_Fl1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/ActivityMainLand_GlHr2"
        app:layout_constraintEnd_toStartOf="@+id/ActivityMainLand_GlVr3"
        app:layout_constraintStart_toStartOf="@+id/ActivityMainLand_GlVr1"
        app:layout_constraintTop_toTopOf="@+id/ActivityMainLand_GlHr1" />

    <FrameLayout
        android:id="@+id/ActivityMainLand_Fl2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/ActivityMainLand_GlHr2"
        app:layout_constraintEnd_toStartOf="@+id/ActivityMainLand_GlVr2"
        app:layout_constraintStart_toStartOf="@+id/ActivityMainLand_GlVr4"
        app:layout_constraintTop_toTopOf="@+id/ActivityMainLand_GlHr1" />

</androidx.constraintlayout.widget.ConstraintLayout>